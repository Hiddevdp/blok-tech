<%- include('../partials/head')%>
    <title>muziek</title>
    </head>

    <body>
        <header>
            <%- include('../partials/header')%>
        </header>
        <main class="muziekpagina">
            <input type="search" placeholder="Zoeken..">
            <section>
                <p>Er zijn <%= ads.length %> advertenties beschikbaar!</p>


                <div id='map' style="display: none"></div>



                <% for ( var index=0; index < ads.length; index++ ) { %>

                    <section>

                        <h1>
                            <%= ads[index].titel %>
                        </h1>
                        <p>
                            <%= ads[index].omschrijving %>
                        </p>
                        <p>
                            <%= ads[index].naam %>
                        </p>
                        <p>
                            <%= ads[index].locatie %>
                        </p>

                    </section>
                    <% } %>

            </section>

        </main>
        <footer>
            <%- include('../partials/nav')%>
        </footer>

    </body>



    </html>